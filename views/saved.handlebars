{{!-- hero/head image --}}
<div class="hero-head">
    <div class="heading-styles">
        <h1>Saved Job Postings</h1>
    </div>
</div>

<div class="notes-header">
    <h2 class="saved-notes-header">Saved Notes</h2>
    <div class="all-saved-notes-container">
        {{#each postings}}
            {{#if this.note}}
                {{this.title}}
                {{#each this.note}}
                    {{ this.note.title}}
                    {{ this.note.body}}
                {{/each}}
            {{/if}}
        {{/each}}
    </div>
    <div class="add-new-note-container hidden" id="notes"></div>
</div>

<div class="job-results-container">
    <h2 class="positions-heading">Saved Positions</h2>
    <div class="job-cards-container">
        {{#each postings}} 
            {{#if this.isSaved}} 
                {{> jobs/saved-job-block}} 
            {{/if}} 
        {{/each}}
    </div>

</div>